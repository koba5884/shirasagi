<dl class="see">
  <dt><%= @model.t :name %></dt>
  <dd><%= @item.send :name %></dd>

  <dt><%= @model.t :term %></dt>
  <dd><%= term(@item) %></dd>

  <% if nil && @item.category %>
  <dt><%= @model.t :category_id %><%= @model.tt :category_id %></dt>
  <dd><%= @item.category.name %></dd>
  <% end %>

  <dt><%= @model.t :target %><%= @model.tt :target %></dt>
  <dd><%= @item.label :target %></dd>
</dl>
